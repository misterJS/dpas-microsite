# Development Environment Configuration
# Used for development/testing environment

# Application Configuration
VITE_APP_NAME=PLS Microsite Frontend
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# API Configuration - Development Backend
VITE_API_BASE_URL=https://api-nprd.prudential.co.id/pls/dev
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_MOCK_API=true
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_DEVTOOLS=true

# External Services - Development
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_ANALYTICS_ID=GA-DEV-XXXXXXX

# Security
VITE_CSRF_TOKEN_HEADER=X-CSRF-Token
VITE_SESSION_TIMEOUT=1800000
VITE_API_KEY=${VITE_API_KEY_DEV}

# UI Configuration
VITE_DEFAULT_LANGUAGE=id
VITE_SUPPORTED_LANGUAGES=id,en
VITE_THEME=default

# Logging
VITE_LOG_LEVEL=debug

# Development Specific
VITE_DEBUG_API_CALLS=true
VITE_SHOW_PERFORMANCE_METRICS=true